{
  "name": "samlify-libxmljs",
  "version": "1.0.6",
  "description": "Samlify validator module powered by libxmljs",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "samlify",
    "validation",
    "validator",
    "saml",
    "xml"
  ],
  "scripts": {
    "build": "tsc && npm run copy",
    "copy": "cpr src/schemas lib/schemas -d",
    "format": "prettier --write \"src/**/*.ts\"",
    "lint": "tslint -p tsconfig.json",
    "postversion": "git push && git push --tags",
    "prepare": "npm run build",
    "prepublishOnly": "npm run lint",
    "preversion": "npm run lint",
    "version": "npm run format && git add -A src"
  },
  "author": "Tom Kirchjunger <tom.kirchjunger@topicus.nl>",
  "license": "MIT",
  "contributors": [
    {
      "name": "Tom Kirchj√ºnger",
      "email": "tom.kirchjunger@topicus.nl",
      "url": "https://github.com/takirchjunger/"
    }
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/takirchjunger/samlify-libxmljs"
  },
  "files": [
    "lib/**/*"
  ],
  "dependencies": {
    "libxmljs": "^0.19.7"
  },
  "devDependencies": {
    "@types/libxmljs": "^0.18.6",
    "@types/node": "^12.12.35",
    "cpr": "^3.0.1",
    "prettier": "^2.0.4",
    "tslint": "^6.1.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.8.3"
  }
}
